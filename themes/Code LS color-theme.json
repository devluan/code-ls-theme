{
	"files.autoSave": "afterDelay", // <== Salvar automaticamente
	"editor.fontFamily": "JetBrains Mono Light", // <== Fontes
	"editor.fontSize": 14, // <== Tamanho da fonte
	"editor.tabSize": 4, // <== Tamanho da tabulação
	"editor.lineHeight": 20, // <== Altura da linha
	"editor.fontLigatures": true, // <== Fonte com ligaduras
	"editor.letterSpacing": -0.0, // Deixar o curso com efeito de 200 FPS
	"editor.cursorBlinking": "expand",
	"editor.renderLineHighlight": "all", // <== Destacar linha atual
  
	"workbench.editor.labelFormat": "default", // <== Formato do nome do arquivo
	"editor.stickyScroll.enabled": true, //habilita uma barra na parte superior de cada item
	"editor.bracketPairColorization.enabled": true,
	"editor.guides.bracketPairs": "active",
	"javascript.updateImportsOnFileMove.enabled": "always",
	"terminal.integrated.defaultProfile.windows": "Git Bash",
	"terminal.integrated.fontFamily": "JetBrains Mono light",
	"explorer.compactFolders": false, // não deixa as pastas juntas
  
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.inlineSuggest.enabled": true,
	"editor.codeActionsOnSave": {
	  "source.fixAll.eslint": "explicit",
	  "source.fixAll": "explicit",
	  "source.organizeImports": "explicit"
	},
	"editor.detectIndentation": true,

